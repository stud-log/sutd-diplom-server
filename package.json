{
  "name": "webpack-express-ts-server",
  "version": "1.0.0",
  "description": "Webpack, Express.js, TypeScript Server",
  "main": "index.js",
  "scripts": {
    "start:dev": "dotenvx run --env-file=.env.development -- nodemon --exec ts-node src/index.ts",
    "start:prod": "dotenvx run --env-file=.env.production -- node dist/server.js",
    "build": "webpack --config webpack.config.ts --mode=production",
    "dev": "npm run start:dev",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@dotenvx/dotenvx": "^0.24.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "^7.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "sequelize": "^6.37.1",
    "sequelize-typescript": "^2.1.6",
    "ts-loader": "^9.2.5",
    "typescript": "^4.5.2"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.6.2",
    "@stylistic/eslint-plugin-js": "^1.6.2",
    "@stylistic/eslint-plugin-ts": "^1.6.2",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.13",
    "@types/node": "^16.9.2",
    "@types/validator": "^13.11.2",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0"
  }
}
