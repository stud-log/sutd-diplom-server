{
  "name": "webpack-express-ts-server",
  "version": "1.0.0",
  "description": "Webpack, Express.js, TypeScript Server",
  "main": "index.js",
  "scripts": {
    "start:dev": "dotenvx run --env-file=.env.development -- nodemon --exec ts-node src/index.ts",
    "start:prod": "dotenvx run --env-file=.env.production -- nodemon --exec ts-node src/index.ts",
    "build": "webpack --config webpack.config.ts --mode=production",
    "dev": "npm run start:dev",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@dotenvx/dotenvx": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "^7.0.1",
    "fast-csv": "^5.0.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.12",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.1",
    "sequelize": "^6.37.1",
    "sequelize-typescript": "^2.1.6",
    "socket.io": "^4.7.5",
    "ts-loader": "^9.2.5",
    "typescript": "^4.5.2",
    "webpack-cli": "^5.1.4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@stud-log/news-types": "^1.1.34",
    "@stylistic/eslint-plugin": "^1.6.2",
    "@stylistic/eslint-plugin-js": "^1.6.2",
    "@stylistic/eslint-plugin-ts": "^1.6.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.0",
    "@types/multer": "^1.4.11",
    "@types/node": "^16.9.2",
    "@types/nodemailer": "^6.4.14",
    "@types/validator": "^13.11.2",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0"
  }
